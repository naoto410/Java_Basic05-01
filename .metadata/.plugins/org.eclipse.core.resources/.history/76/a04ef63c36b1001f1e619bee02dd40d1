package mypackage;

import java.io.BufferedReader;
import java.io.FileReader;

public class index {
    public static void main(String[] args) {
    	try {
        	BufferedReader br = new BufferedReader(new FileReader("originalWord.txt"));
        	String word = br.readLine();
        	System.out.println(word);
    	} catch(IOException e) {
    		e.printStackTrace();
    	}
    }
}